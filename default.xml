<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<remote name="96boards" fetch="https://github.com/96boards" />
	<remote name="96boards-hikey" fetch="https://github.com/96boards-hikey" />
	<remote name="busybox" fetch="git://busybox.net" />
	<remote name="landing-teams" fetch="https://git.linaro.org/landing-teams" />
	<remote name="linaro" fetch="https://git.linaro.org" />
	<remote name="linaro-swg" fetch="https://github.com/linaro-swg" />
	<remote name="linux" fetch="https://git.kernel.org/pub/scm/linux/kernel/git/torvalds" />
	<remote name="optee" fetch="https://github.com/OP-TEE" />
	<remote name="qemu" fetch="https://github.com/qemu" />
	<remote name="savannah" fetch="git://git.savannah.gnu.org" />
	<remote name="sfnet" fetch="git://git.code.sf.net/p/strace" />
	<remote name="tianocore" fetch="https://github.com/tianocore" />
	<remote name="u-boot" fetch="git://git.denx.de" />

	<default remote="optee" revision="master" />

	<!-- OP-TEE gits -->
	<project path="optee_os" name="optee_os.git" />
	<project path="optee_client" name="optee_client.git" />
	<project path="optee_test" name="optee_test.git" />

	<!-- busybox -->
	<project remote="busybox" path="busybox" name="busybox.git" revision="dbf5a6da6a4295ce26edd1ce34fde567d19afa02" />

	<!-- ARM gits, FVP -->
	<project remote="linaro-swg" path="arm-trusted-firmware" name="arm-trusted-firmware.git" revision="optee_paged_armtf_v1.2" />

	<!-- Recovery files -->
	<project remote="linaro" path="vexpress-firmware" name="arm/vexpress-firmware.git" revision="cad383a2c4ffeb9192605fda54c07fd1aa09a1b0"/>

	<!-- Tianocore, EDK2 -->
	<!-- <project remote="96boards-hikey" path="edk2" name="edk2.git" revision="testing/hikey960_v2.5" /> -->
	<project remote="96boards-hikey" path="OpenPlatformPkg" name="OpenPlatformPkg" revision="testing/hikey960_v1.3.4" />
	<project remote="tianocore" path="edk2" name="edk2.git" />
	<project remote="tianocore" path="edk2-platforms" name="edk2-platforms.git" />

	<!-- l-loader -->
	<project remote="96boards" path="l-loader" name="l-loader" />

	<!-- burn-boot -->
	<project remote="96boards" path="burn-boot" name="burn-boot" />


	<!-- Linux kernel -->
	<project remote="linaro-swg" path="linux" name="linux.git" revision="optee"/>

	<!-- U-Boot -->
	<project remote="u-boot" path="u-boot" name="u-boot.git" />

	<!-- strace -->
	<project remote="sfnet" path="strace" name="code" />

	<!-- grub -->
	<project remote="savannah" path="grub" name="grub" />

	<!-- patches -->
	<project remote="linaro-swg" path="patches_hikey" name="patches_hikey" />

	<!-- linaro-swg gits -->
	<project remote="linaro-swg" path="gen_rootfs" name="gen_rootfs.git" />
	<project remote="linaro-swg" path="soc_term" name="soc_term.git" />
	<project remote="linaro-swg" path="bios_qemu_tz_arm" name="bios_qemu_tz_arm.git" />
	<project remote="linaro-swg" path="sha-perf" name="sha-perf" />
	<project remote="linaro-swg" path="hello_world" name="hello_world.git" />
	<project remote="linaro-swg" path="optee_examples" name="optee_examples.git" />

	<!-- QEMU -->
	<project remote="qemu" path="qemu" name="qemu.git" />

	<!-- MediaTek -->
	<project remote="linaro-swg" path="mtk_tools" name="mtk_tools.git" />
	<project remote="linaro-swg" path="evb-utils" name="evb-utils.git" />

	<!-- Build -->
	<project remote="optee" path="build" name="build.git">
		<linkfile src="qemu.mk" dest="build/Makefile" />
	</project>
</manifest>
